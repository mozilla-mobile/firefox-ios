# The configuration for the menuRefactorFeature feature
features:
  menu-refactor-feature:
    description: >
      Controls the menu refactor feature
    variables:
     enabled:
        description: >
          Controls which menu users will see
        type: Boolean
        default: false
    defaults:
      - channel: beta
        value:
          new-variable: false
      - channel: developer
        value:
          new-variable: false
