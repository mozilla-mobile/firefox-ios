      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="6.4" build-num="6E35b"/>
    </header>
    <body>
      <trans-unit id="Available devices:">
        <source>Available devices:</source>
        <target>Gléasanna ar fáil:</target>
        <note>Header for the list of devices table</note>
      </trans-unit>
      <trans-unit id="Close">
        <source>Close</source>
        <target>Dún</target>
        <note>Close button in top navigation bar</note>
      </trans-unit>
      <trans-unit id="Please open Firefox, go to Settings and sign in to continue.">
        <source>Please open Firefox, go to Settings and sign in to continue.</source>
        <target>Oscail Firefox, ansin na Socruithe, agus logáil isteach le dul ar aghaidh.</target>
        <note>See http://mzl.la/1ISlXnU</note>
      </trans-unit>
      <trans-unit id="Send">
        <source>Send</source>
        <target>Seol</target>
        <note>Navigation bar button to Send the current page to a device</note>
      </trans-unit>
      <trans-unit id="Send Tab">
        <source>Send Tab</source>
        <target>Seol Cluaisín</target>
        <note>Title of the dialog that allows you to send a tab to a different device</note>
      </trans-unit>
      <trans-unit id="You are not signed in to your Firefox Account.">
        <source>You are not signed in to your Firefox Account.</source>
        <target>Níl tú logáilte isteach i do chuntas Firefox.</target>
        <note>See http://mzl.la/1ISlXnU</note>
      </trans-unit>
      <trans-unit id="You don't have any other devices connected to this Firefox Account available to sync.">
        <source>You don't have any other devices connected to this Firefox Account available to sync.</source>
        <target>Níl aon ghléasanna eile ceangailte leis an gCuntas Firefox seo atá ar fáil le sioncrónú.</target>
        <note>Error message shown in the remote tabs panel</note>
      </trans-unit>
    </body>
  </file>
