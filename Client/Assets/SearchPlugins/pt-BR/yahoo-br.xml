<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Yahoo</ShortName>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" method="GET" template="https://br.search.yahoo.com/sugg/ff">
  <Param name="output" value="fxjson"/>
  <Param name="appid" value="ffm"/>
  <Param name="command" value="{searchTerms}"/>
  <MozParam name="nresults" condition="pref" pref="maxSuggestions"/>
</Url>
<Url type="text/html" method="GET" template="https://br.search.yahoo.com/search">
  <Param name="p" value="{searchTerms}"/>
  <Param name="ei" value="UTF-8"/>
  <Param name="fr" value="mozmob1"/>
  <Param name="type" value="ios"/>
</Url>
<SearchForm>https://br.search.yahoo.com/</SearchForm>
</SearchPlugin>
